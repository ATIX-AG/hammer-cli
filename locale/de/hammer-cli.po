#: lib/hammer_cli/clamp.rb:5
msgid "too many arguments"
msgstr "Zu viele Argumente"

#: lib/hammer_cli/clamp.rb:6
msgid "option '%<option>s' is required"
msgstr "Option \"%<option>s\" ist erforderlich"

